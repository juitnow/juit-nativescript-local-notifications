{
  "name": "@juit/nativescript-local-notifications",
  "version": "1.2.0",
  "description": "Minimalistic local notifications for NativeScript 7",
  "main": "dist/local-notifications",
  "types": "dist/index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    }
  },
  "scripts": {
    "build": "rm -rf dist && eslint src/ && tsc && cp src/index.d.ts dist/",
    "compile": "tsc",
    "lint": "eslint src/",
    "lintfix": "eslint --fix src/",
    "prepare": "npm install --no-audit --no-save '@nativescript/webpack@<5' && ts-patch install"
  },
  "author": "Pier Fumagalli <pier.fumagalli@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@nativescript/core": "<8",
    "@nativescript/types-android": "<8",
    "@nativescript/types-ios": "^8",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "ts-node": "^9.1.1",
    "ts-patch": "^1.3.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@nativescript/shared-notification-delegate": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juitnow/juit-nativescript-local-notifications.git"
  },
  "keywords": [
    "ecosystem:NativeScript",
    "NativeScript",
    "Notifications",
    "Local Notifications"
  ],
  "bugs": {
    "url": "https://github.com/juitnow/juit-nativescript-local-notifications/issues"
  },
  "homepage": "https://github.com/juitnow/juit-nativescript-local-notifications#readme"
}
