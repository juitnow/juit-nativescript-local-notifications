{
  "name": "@juit/nativescript-local-notifications",
  "version": "1.0.1",
  "description": "Minimalistic local notifications for NativeScript 7",
  "main": "dist/local-notifications",
  "types": "dist/index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    }
  },
  "scripts": {
    "build": "rm -rf dist && eslint src/ && tsc && cp src/index.d.ts dist/",
    "compile": "tsc",
    "lint": "eslint src/",
    "lintfix": "eslint --fix src/",
    "prepare": "ts-patch install"
  },
  "author": "Pier Fumagalli <pier.fumagalli@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@nativescript/core": "^7.2.1",
    "@nativescript/types-android": "^7.2.0",
    "@nativescript/types-ios": "^7.2.0",
    "@nativescript/webpack": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "ts-node": "^9.1.1",
    "ts-patch": "^1.3.2",
    "typescript": "^4.2.2"
  }
}
